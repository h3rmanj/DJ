<Window x:Class="DJ_Hermy_2._0.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DJ_Hermy_2._0"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="600">
	<Grid>
		<TextBox x:Name="TeamSpeakIP" HorizontalAlignment="Left" Margin="100,100,0,0" Text="" VerticalAlignment="Top" Width="400" Height="32"/>
		<TextBox x:Name="ServerQueryLogin" HorizontalAlignment="Left" Margin="100,150,0,0" Text="" VerticalAlignment="Top" Width="400" Height="32"/>
		<PasswordBox x:Name="ServerQueryPassword" HorizontalAlignment="Left" Margin="100,200,0,0" VerticalAlignment="Top" Width="400" Height="32"/>
		<Button x:Name="LoginButton" Content="Login" HorizontalAlignment="Left" Margin="200,250,0,0" VerticalAlignment="Top" Width="200" Height="32" Click="LoginButton_Click"/>
	</Grid>
</Window>
